    "lint:style": "stylelint './**/*.{ts,tsx}'",
    "lint:text": "textlint './**/*.{ts,tsx}'",
    "textlint": "textlint src/**/*.ts{,x}",
    "lint-fix": "run-s lint-fix:*",
    "lint-fix:eslint": "eslint --fix './**/*.{ts,tsx}'",
    "lint-fix:prettier": "prettier --write './**/*.{ts,tsx}'",
    "lint-fix:style": "stylelint --fix './**/*.{ts,tsx}'",
    "lint-fix:text": "textlint --fix './**/*.{ts,tsx}'",
    "test": "react-router typegen && tsc --noEmit && vitest --run",
